{
  "name": "test-task-full",
  "version": "1.0.0",
  "description": "react, redux, typescript, mongoDB, express, nodeJS",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "start:watch": "nodemon"
  },
  "author": "Roman Kruglyanski <eviltroll@yandex.ru>",
  "license": "MIT",
  "private": false,
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "dependencies": {
    "config": "^3.3.3",
    "express": "^4.17.1",
    "git": "^0.1.5",
    "mongoose": "^5.11.17"
  },
  "devDependencies": {
    "@types/concurrently": "^5.2.1",
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.11",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.31",
    "@types/nodemon": "^1.19.0",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
